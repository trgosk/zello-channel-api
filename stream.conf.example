# Configuration for Zello Streaming API Example
# In order to generate an auth token follow:
# https://github.com/zelloptt/zello-channel-api/blob/master/AUTH.md

[zello]
username = zello_username
password = zello_password
channel = zello_channel
issuer = zello_issuer
privatekeyfile = zello.private.key

[media]
sink = Virtual1.monitor
filename = sample.opus

[zmq]
address = tcp://127.0.0.1:5556